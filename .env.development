# Development Environment Configuration
NODE_ENV=development

# Application URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000
EXPO_PUBLIC_APP_URL=http://localhost:3000

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_PROJECT_ID=your-project-id

# Expo Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
EXPO_PUBLIC_ENVIRONMENT=development
EXPO_PUBLIC_APP_VARIANT=development

# Authentication
NEXTAUTH_SECRET=your-nextauth-secret-for-development
NEXTAUTH_URL=http://localhost:3000

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/starter_template_dev

# Redis (Optional)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Email (Development)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=noreply@localhost

# File Storage
STORAGE_BUCKET=uploads-dev
STORAGE_REGION=us-east-1

# API Configuration
API_RATE_LIMIT=1000
API_RATE_WINDOW=900000

# Logging
LOG_LEVEL=debug
LOG_FORMAT=pretty

# Feature Flags
FEATURE_ANALYTICS=false
FEATURE_PAYMENTS=false
FEATURE_NOTIFICATIONS=true

# External Services (Development)
STRIPE_PUBLIC_KEY=pk_test_your-stripe-public-key
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-stripe-webhook-secret

# Analytics (Disabled in development)
GOOGLE_ANALYTICS_ID=
MIXPANEL_TOKEN=
POSTHOG_KEY=

# Monitoring (Development)
SENTRY_DSN=
SENTRY_ENVIRONMENT=development

# Cron Jobs
CRON_SECRET=development-cron-secret

# Development Tools
NEXT_PUBLIC_SHOW_DEBUG_INFO=true
EXPO_PUBLIC_SHOW_DEBUG_INFO=true
STORYBOOK_ENABLED=true