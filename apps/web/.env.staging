# Staging Environment Configuration
# This file contains staging-specific environment variables

# Application
NODE_ENV=staging
NEXT_PUBLIC_APP_URL=https://staging.starter-template.com
NEXT_PUBLIC_API_URL=https://staging.starter-template.com/api

# Staging Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-staging-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-staging-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-staging-service-role-key

# Staging Database
DATABASE_URL=postgresql://postgres:password@staging-db:5432/starter_template_staging

# Staging Authentication
NEXTAUTH_URL=https://staging.starter-template.com
NEXTAUTH_SECRET=staging-secret-key-very-secure

# Staging Features
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=true
NEXT_PUBLIC_ENABLE_DEVTOOLS=false
NEXT_PUBLIC_ENABLE_BETA_FEATURES=true

# Staging Monitoring
NEXT_PUBLIC_SENTRY_DSN=https://your-staging-sentry-dsn
SENTRY_ORG=your-org
SENTRY_PROJECT=starter-template-staging

# Staging Analytics
NEXT_PUBLIC_POSTHOG_KEY=your-staging-posthog-key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Email Service
EMAIL_FROM=staging@starter-template.com
RESEND_API_KEY=your-staging-resend-key

# OAuth
GOOGLE_CLIENT_ID=your-staging-google-client-id
GOOGLE_CLIENT_SECRET=your-staging-google-client-secret

# Tools
NEXT_TELEMETRY_DISABLED=1
DEBUG=false