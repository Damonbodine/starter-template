# Mobile Production Environment Configuration

# Application
NODE_ENV=production
EXPO_PUBLIC_APP_SCHEME=starter-template
EXPO_PUBLIC_API_URL=https://starter-template.com/api
EXPO_PUBLIC_WEB_URL=https://starter-template.com

# Production Supabase
EXPO_PUBLIC_SUPABASE_URL=https://your-production-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-production-anon-key

# Expo Configuration
EXPO_PROJECT_ID=your-expo-project-id
EAS_BUILD_PROFILE=production

# Production Features
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true
EXPO_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true
EXPO_PUBLIC_ENABLE_DEVTOOLS=false
EXPO_PUBLIC_ENABLE_FLIPPER=false
EXPO_PUBLIC_ENABLE_BETA_FEATURES=false
EXPO_PUBLIC_ENABLE_EXPERIMENTAL_UI=false

# Platform Features
EXPO_PUBLIC_ENABLE_IOS_FEATURES=true
EXPO_PUBLIC_ENABLE_ANDROID_FEATURES=true
EXPO_PUBLIC_ENABLE_LOCATION_SERVICES=true
EXPO_PUBLIC_ENABLE_CAMERA=true
EXPO_PUBLIC_ENABLE_MEDIA_LIBRARY=true
EXPO_PUBLIC_ENABLE_BIOMETRIC_AUTH=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true

# Permissions
EXPO_PUBLIC_REQUIRE_CAMERA_PERMISSION=true
EXPO_PUBLIC_REQUIRE_LOCATION_PERMISSION=true
EXPO_PUBLIC_REQUIRE_NOTIFICATION_PERMISSION=true
EXPO_PUBLIC_REQUIRE_CONTACTS_PERMISSION=false

# Monitoring
EXPO_PUBLIC_SENTRY_DSN=https://your-production-sentry-dsn

# Analytics
EXPO_PUBLIC_POSTHOG_KEY=your-production-posthog-key
EXPO_PUBLIC_AMPLITUDE_API_KEY=your-production-amplitude-key
EXPO_PUBLIC_MIXPANEL_TOKEN=your-production-mixpanel-token

# Maps
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-production-maps-key
EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN=your-production-mapbox-token

# Push Notifications
EXPO_PUSH_TOKEN=your-production-push-token
EXPO_PUBLIC_FCM_SENDER_ID=your-production-fcm-sender-id

# Apple Push Notifications
APNS_KEY_ID=your-production-apns-key-id
APNS_TEAM_ID=your-production-team-id

# Social Media
EXPO_PUBLIC_FACEBOOK_APP_ID=your-production-facebook-app-id

# Payment
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your-stripe-key
EXPO_PUBLIC_REVENUE_CAT_API_KEY=your-production-revenue-cat-key

# OAuth
EXPO_PUBLIC_OAUTH_REDIRECT_SCHEME=starter-template

# Background Tasks
EXPO_PUBLIC_ENABLE_BACKGROUND_SYNC=true
EXPO_PUBLIC_BACKGROUND_SYNC_INTERVAL=300000

# Build Information
APP_VERSION=1.0.0
BUILD_NUMBER=1

# Tools
EXPO_PUBLIC_DEBUG=false