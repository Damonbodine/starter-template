# Mobile Staging Environment Configuration

# Application
NODE_ENV=staging
EXPO_PUBLIC_APP_SCHEME=starter-template-staging
EXPO_PUBLIC_API_URL=https://staging.starter-template.com/api
EXPO_PUBLIC_WEB_URL=https://staging.starter-template.com

# Staging Supabase
EXPO_PUBLIC_SUPABASE_URL=https://your-staging-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-staging-anon-key

# Expo Configuration
EXPO_PROJECT_ID=your-expo-project-id
EAS_BUILD_PROFILE=staging

# Staging Features
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true
EXPO_PUBLIC_ENABLE_DEVTOOLS=false
EXPO_PUBLIC_ENABLE_FLIPPER=false
EXPO_PUBLIC_ENABLE_BETA_FEATURES=true

# Platform Features
EXPO_PUBLIC_ENABLE_LOCATION_SERVICES=true
EXPO_PUBLIC_ENABLE_CAMERA=true
EXPO_PUBLIC_ENABLE_BIOMETRIC_AUTH=true

# Permissions
EXPO_PUBLIC_REQUIRE_CAMERA_PERMISSION=true
EXPO_PUBLIC_REQUIRE_LOCATION_PERMISSION=true
EXPO_PUBLIC_REQUIRE_NOTIFICATION_PERMISSION=true

# Monitoring
EXPO_PUBLIC_SENTRY_DSN=https://your-staging-sentry-dsn

# Analytics
EXPO_PUBLIC_POSTHOG_KEY=your-staging-posthog-key
EXPO_PUBLIC_AMPLITUDE_API_KEY=your-staging-amplitude-key

# Maps
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-staging-maps-key

# Push Notifications
EXPO_PUSH_TOKEN=your-staging-push-token
EXPO_PUBLIC_FCM_SENDER_ID=your-staging-fcm-sender-id

# OAuth
EXPO_PUBLIC_OAUTH_REDIRECT_SCHEME=starter-template-staging

# Background Tasks
EXPO_PUBLIC_ENABLE_BACKGROUND_SYNC=true
EXPO_PUBLIC_BACKGROUND_SYNC_INTERVAL=300000

# Tools
EXPO_PUBLIC_DEBUG=false