# Staging Environment Configuration
NODE_ENV=staging

# Application URLs
NEXT_PUBLIC_APP_URL=https://starter-template-staging.vercel.app
EXPO_PUBLIC_APP_URL=https://starter-template-staging.vercel.app

# Supabase Configuration (Staging)
NEXT_PUBLIC_SUPABASE_URL=https://your-staging-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-staging-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-staging-supabase-service-role-key
SUPABASE_PROJECT_ID=your-staging-project-id

# Expo Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-staging-project-id.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-staging-supabase-anon-key
EXPO_PUBLIC_ENVIRONMENT=staging
EXPO_PUBLIC_APP_VARIANT=staging

# Authentication
NEXTAUTH_SECRET=your-nextauth-secret-for-staging
NEXTAUTH_URL=https://starter-template-staging.vercel.app

# Database
DATABASE_URL=postgresql://postgres:your-password@your-staging-db-host:5432/starter_template_staging

# Redis (Staging)
REDIS_URL=redis://your-staging-redis-host:6379
REDIS_PASSWORD=your-redis-password

# Email (Staging - using real SMTP)
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_PASSWORD=your-sendgrid-api-key
SMTP_FROM=noreply@staging.yourapp.com

# File Storage
STORAGE_BUCKET=uploads-staging
STORAGE_REGION=us-east-1

# API Configuration
API_RATE_LIMIT=500
API_RATE_WINDOW=900000

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Feature Flags
FEATURE_ANALYTICS=true
FEATURE_PAYMENTS=false
FEATURE_NOTIFICATIONS=true

# External Services (Test keys)
STRIPE_PUBLIC_KEY=pk_test_your-staging-stripe-public-key
STRIPE_SECRET_KEY=sk_test_your-staging-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-staging-stripe-webhook-secret

# Analytics (Staging)
GOOGLE_ANALYTICS_ID=GA-STAGING-ID
MIXPANEL_TOKEN=your-staging-mixpanel-token
POSTHOG_KEY=your-staging-posthog-key

# Monitoring (Staging)
SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/your-project
SENTRY_ENVIRONMENT=staging

# Cron Jobs
CRON_SECRET=staging-cron-secret-should-be-secure

# Debug (Limited in staging)
NEXT_PUBLIC_SHOW_DEBUG_INFO=false
EXPO_PUBLIC_SHOW_DEBUG_INFO=false
STORYBOOK_ENABLED=true